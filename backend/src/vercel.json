{
    "version": 2,
    "name": "PakFood",
    "builds": [
      {
        "src": "src/server.ts", 
        "use": "@vercel/node"
      },
      {
        "src": "angular.json", 
        "use": "@angular-builders/custom-webpack:browser"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "src/server.ts" },
      { "src": "/(.*)", "dest": "built/browser/$1" }
    ]
  }